{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "semi": "error",
        "no-unused-vars": "warn",
        "quotes":"error",
        "comma-spacing":"error",
        "no-trailing-spaces":"error"
    }
}